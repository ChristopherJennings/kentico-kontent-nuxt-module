{
  "name": "kentico-kontent-nuxt-module",
  "version": "1.0.0-release",
  "description": "Add Kentico Kontent super power to your nuxt app",
  "main": "lib/module.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "rollup -c",
    "test": "mocha tests/*.js --require @babel/register --reporter spec || exit 0",
    "posttest": "standard lib/module.js lib/templates/plugin.template.js lib/services/*.js tests/*.js --fix"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Domitnator/kentico-kontent-nuxt-module.git"
  },
  "keywords": [
    "nuxt",
    "kentico",
    "kontent",
    "vuejs",
    "nuxtjs"
  ],
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "author": "Alfred Brockotter",
  "contributors": [
    {
      "name": "Alfred Brockotter <alfred@domitnator.nl>"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Domitnator/kentico-kontent-nuxt-module/issues"
  },
  "homepage": "https://github.com/Domitnator/kentico-kontent-nuxt-module#readme",
  "dependencies": {
    "@kentico/kontent-delivery": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@semantic-release/npm": "^5.2.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "standard": "*",
    "mocha": "^6.2.1",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "semantic-release": "^15.13.24"
  }
}
